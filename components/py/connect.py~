#!/usr/bin/python3
# -*- coding: utf-8 -*-

import urllib.request as web

class TheNet():

	def webConnect(url):
		try:
			obj=web.urlopen(url)
			if(obj.code==200):
				return obj.code
		except web.URLError as e:
			print(e)
			print("Network connection Error")



def func(text):
	t= " So happy do be here..!"
	#print(text.something(4,5))
	return text +t
